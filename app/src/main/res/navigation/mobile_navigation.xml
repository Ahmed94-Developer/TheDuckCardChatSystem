<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.theduckcardchatsystem.ui.home.HomeFragment"
        android:label="@string/title_chats"
        tools:layout="@layout/fragment_home" >

    </fragment>

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.theduckcardchatsystem.ui.contacts.ContactsFragment"
        android:label="@string/title_contacts"
        tools:layout="@layout/fragment_contacts" >

    </fragment>

    <fragment
        android:id="@+id/navigation_group"
        android:name="com.example.theduckcardchatsystem.ui.groups.AddContactsFragment"
        android:label="@string/title_create_group"
        tools:layout="@layout/fragment_add_contacts" >

    </fragment>
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.theduckcardchatsystem.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/navigation_change_name"
        android:name="com.example.theduckcardchatsystem.ui.settings.ChangeNameFragment"
        android:label="@string/title_create_group"
        tools:layout="@layout/fragment_change_name" >

    </fragment>
</navigation>
